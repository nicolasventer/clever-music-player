{
	"name": "clever-music-player-server",
	"type": "module",
	"scripts": {
		"devEnv": "echo 'export const B_PROD = false;' > env.ts",
		"prodEnv": "echo 'export const B_PROD = true;' > env.ts",
		"dev": "bun run devEnv && bun run index.ts",
		"prod": "bun run prodEnv && bun run index.ts",
		"copy": "cp -R ../client/dist ."
	},
	"devDependencies": {
		"@types/bun": "1.2.20"
	},
	"peerDependencies": {
		"typescript": "5"
	},
	"dependencies": {
		"webview-bun": "2.4.0"
	}
}
